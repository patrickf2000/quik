# extern.qk
# Provides an example of how we can use external functions

# List the functions
extern func puts(s:str)
extern func system(s:str)
extern func exit(code:int)

# The main function
func main
	puts("Calling the LS command.")
	system("ls -l")
	
	puts("")
	puts("Exiting with code of 2.")
	
	exit(2)
end
