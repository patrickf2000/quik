# func_call.qk
# Provides an example of different function calls

func sayHello
	println("Hello!")
end

func add_int(n1:int, n2:int)
	int answer = n1 + n2
	return answer
end

func add_float(n1:float, n2:float)
	float answer = n1 + n2
	return answer
end

func main
	sayHello()
	
	int answer1 = add_int(2,3)
	println(answer1)
	
	float answer2 = add_int(1.4, 2.33)
	println(answer2)
end
