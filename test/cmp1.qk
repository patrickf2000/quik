include io

func cmp_int(x:int)
	println("X: ", x)
	
	if (x == 10)
		println("Equal to 10!")
	elif (x == 20)
		println("Equal to 20!")
	else
		println("I dont know!")
	end
	
	println("")
end

func main
	int x = 20
	cmp_int(x)
	
	x = 10
	cmp_int(x)
	
	x = 11
	cmp_int(x)
	
	println("Done")
end

