include stdio
INCLUDE~ID>stdio~

func main
FUNC_DEC~ID>main~

func main()
FUNC_DEC~ID>main~LEFT_PAREN~RIGHT_PAREN~

puts(x)
ID>puts~LEFT_PAREN~ID>x~RIGHT_PAREN~

puts((x))
ID>puts~LEFT_PAREN~LEFT_PAREN~ID>x~RIGHT_PAREN~RIGHT_PAREN~

puts("Hello!")
ID>puts~LEFT_PAREN~STRING>Hello!~RIGHT_PAREN~

end
END~

x:int
ID>x~COLON~T_INT~

func myFunc(x:int)
FUNC_DEC~ID>myFunc~LEFT_PAREN~ID>x~COLON~T_INT~RIGHT_PAREN~

func myFunc(x:int, y:int)
FUNC_DEC~ID>myFunc~LEFT_PAREN~ID>x~COLON~T_INT~COMMA~ID>y~COLON~T_INT~RIGHT_PAREN~

extern func exit(code:int)
EXTERN~FUNC_DEC~ID>exit~LEFT_PAREN~ID>code~COLON~T_INT~RIGHT_PAREN~

if (x == y)
IF~LEFT_PAREN~ID>x~EQUALS~ID>y~RIGHT_PAREN~

elif (x == y)
ELIF~LEFT_PAREN~ID>x~EQUALS~ID>y~RIGHT_PAREN~

else
ELSE~

== != > < >= <=
EQUALS~NOT_EQUAL~GREATER~LESS~GREATER_EQ~LESS_EQ~
