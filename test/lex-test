include stdio
INCLUDE~ID>stdio~

func main
FUNC_DEC~ID>main~

func main()
FUNC_DEC~ID>main~LEFT_PAREN~RIGHT_PAREN~

puts(x)
ID>puts~LEFT_PAREN~ID>x~RIGHT_PAREN~

puts((x))
ID>puts~LEFT_PAREN~LEFT_PAREN~ID>x~RIGHT_PAREN~RIGHT_PAREN~

puts("Hello!")
ID>puts~LEFT_PAREN~STRING>Hello!~RIGHT_PAREN~

end
END~

x:int
ID>x~COLON~T_INT~

func func(x:int)
FUNC_DEC~ID>func~LEFT_PAREN~ID>x~COLON~T_INT~RIGHT_PAREN~

func func(x:int, y:int)
FUNC_DEC~ID>func~LEFT_PAREN~ID>x~COLON~T_INT~COMMA~ID>y~COLON~T_INT~RIGHT_PAREN~
