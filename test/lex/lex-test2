byte x = 0xA1
T_BYTE~ID>x~ASSIGN~HEX>0xA1~

char x = 'a'
T_CHAR~ID>x~ASSIGN~CHAR>a~

short x = 1
T_SHORT~ID>x~ASSIGN~NO>1~

int x = 32
T_INT~ID>x~ASSIGN~NO>32~

float pi = 3.14
T_FLOAT~ID>pi~ASSIGN~DEC>3.14~

bool x = true
T_BOOL~ID>x~ASSIGN~B_TRUE~

bool x = false
T_BOOL~ID>x~ASSIGN~B_FALSE~

str s = "Hello!"
T_STR~ID>s~ASSIGN~STRING>Hello!~

int[3] array = 1, 2, 3
T_INT~L_BRACKET~NO>3~R_BRACKET~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~

float128 array = 1, 2, 3, 4
FLOAT_128~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~

float256 array = 1, 2, 3, 4, 5, 6, 7, 8
FLOAT_256~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~,~NO>5~,~NO>6~,~NO>7~,~NO>8~

float80 pi = 3.14
FLOAT_80~ID>pi~ASSIGN~DEC>3.14~

int128 array = 1, 2, 3, 4
INT_128~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~

int256 array = 1, 2, 3, 4, 5, 6, 7, 8
INT_256~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~,~NO>5~,~NO>6~,~NO>7~,~NO>8~

double pi = 3.14
T_DOUBLE~ID>pi~ASSIGN~DEC>3.14~

double128 array = 1, 2
DOUBLE_128~ID>array~ASSIGN~NO>1~,~NO>2~

double256 array = 1, 2, 3, 4
DOUBLE_256~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~

uint x = 32
T_UINT~ID>x~ASSIGN~NO>32~

uint128 array = 1, 2, 3, 4
UINT_128~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~

uint256 array = 1, 2, 3, 4, 5, 6, 7, 8
UINT_256~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~,~NO>5~,~NO>6~,~NO>7~,~NO>8~

