byte x = 1
T_BYTE~ID>x~ASSIGN~NO>1~

char x = 'a'
T_CHAR~ID>x~ASSIGN~CHAR>a~

short x = 1
T_SHORT~ID>x~ASSIGN~NO>1~

int x = 32
T_INT~ID>x~ASSIGN~NO>32~

long x = 64
T_LONG~ID>x~ASSIGN~NO>64~

float pi = 3.14
T_FLOAT~ID>pi~ASSIGN~DEC>3.14~

bool x = true
T_BOOL~ID>x~ASSIGN~B_TRUE~

bool x = false
T_BOOL~ID>x~ASSIGN~B_FALSE~

str s = "Hello!"
T_STR~ID>s~ASSIGN~STRING>Hello!~

var x = 32
VAR~ID>x~ASSIGN~NO>32~

int[3] array = 1, 2, 3
T_INT~L_BRACKET~NO>3~R_BRACKET~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~

float128 array = 1, 2, 3, 4
FLOAT_128~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~

float256 array = 1, 2, 3, 4, 5, 6, 7, 8
FLOAT_256~ID>array~ASSIGN~NO>1~,~NO>2~,~NO>3~,~NO>4~,~NO>5~,~NO>6~,~NO>7~,~NO>8~

float80 pi = 3.14
FLOAT_80~ID>pi~ASSIGN~DEC>3.14~
