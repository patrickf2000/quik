# int_arr1
# Integer arrays
include io

func main
	int[5] numbers = 1, 7, 2, 4, 1
	int x = numbers[2]
	
	print_int("Element at index 2: ", x)
	
	println("")
	println("Counting all elements in array:")
	
	int index = 0
	while (index < 5)
		x = numbers[index]
		print_int("", x)
		index = index + 1
	end
end
